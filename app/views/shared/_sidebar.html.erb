<div class="sidebar">
  <div class="avatar-div d-flex align-items-center">
    <div class="avatar-img">
      <% if current_user.avatar.present? %>
        <%= image_tag current_user.avatar, class: "avatar-large" %>
      <% else %>
        <img class="avatar-large" alt="avatar-large" src="data:image/png;base64," />
      <% end %>
    </div>
    <div class="avatar-info">
      <p><%= "Software Developer".upcase %></p>
      <% if user_signed_in? %>
        <p><strong><%= current_user.username %></strong></p>
      <% end %>
    </div>
  </div>
  <ul class="sidebar-list" data-controller="add">
    <%# Dashboard %>
    <li class="" data-action="load->add#load" data-add-target="pageName" data-page="<%= dashboard_path %>">
      <i class="fa-solid fa-chart-pie sidebar-icon"></i><%= link_to "Dashboard", dashboard_path %>
    </li>

    <%# Planner %>
    <li data-action="load->add#load" data-add-target="pageName" data-page="<%= tasks_path %>">
      <i class="fa-solid fa-calendar-days sidebar-icon"></i><%= link_to "Planner", tasks_path %></li>

    <%# Reminders %>
    <li data-action="load->add#load" data-add-target="pageName" data-page="<%= alerts_path %>">
      <i class="fa-solid fa-bell sidebar-icon"></i><%= link_to "Reminders", alerts_path %>
    </li>

    <%# <li><i class="fa-solid fa-sitemap sidebar-icon"></i><%= link_to "Projects", projects_path</li> %>

    <%# Chat %>
    <li data-action="load->add#load" data-add-target="pageName" data-page="<%= chatroom_path(Chatroom.first) %>">
      <i class="fa-solid fa-comments sidebar-icon"></i>
      <% if current_user.chatrooms.any? %>
        <%= link_to "Chat",chatroom_path(current_user.chatrooms.last) %>
        <% else %>
        <%= link_to "Chat",chatroom_path(Chatroom.first) %>
      <% end %>
    </li>

    <%# Music %>
    <li class="" data-action="load->add#load" data-add-target="pageName" data-page="<%= music_path %>" >
      <i class="fa-solid fa-music sidebar-icon"></i><%= link_to "Music", music_path %></li>

    <%# <li><a href="#"><i class="fa-regular fa-map-location-dot sidebar-icon"></i>Offices</a></li>
    <li class="pb-4 border-bottom"><a href="#"><i class="fa-regular fa-comments sidebar-icon"></i>Chat</a></li> %>
    <li class="pt-4"><i class="fa-solid fa-right-from-bracket sidebar-logout text-danger"></i><%= link_to "Logout", destroy_user_session_path, data: {turbo_method: :delete}, class: "text-danger" %></li>
  </ul>
</div>
