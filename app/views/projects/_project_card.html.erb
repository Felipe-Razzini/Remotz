<div class="row">
  <% @projects.each do |project| %>
  <div class="col-3">
    <div class="card" style="width: 15rem; height: 21rem;">
      <img class="card-img-top" style="height: 7rem;"src="data:image/jpeg;base64," alt="Card image cap">
      <div class="card-body">
        <h5 class="card-title"><strong><%= project.name %></strong></h5>
        <p class="card-text"></p> Start date: <%= project.start_date %>
        <p class="card-text"></p> End date: <%= project.end_date %>

        <div class="project-avatar">
          <div>
            <img class="avatar" alt="avatar" src="https://kitt.lewagon.com/placeholder/users/cveneziani" />
          </div>
          <div>
            <p class="m-0">Name</p>
          </div>
        </div>
      </div>
    </div>
  </div>
  <% end %>
</div>
