<div class="container">
  <%= link_to "update", edit_task_path(@task), class: "btn btn-primary rounded"%>
  <%= link_to "All tasks", tasks_path(@task), class: "btn btn-primary rounded"%>
  <h2><%= @task.title %></h2>
  <h2><%= @task.start%></h2>
  <h2><%= @task.priority%></h2>
  <h2><%= @task.end%></h2>
  <h2><%= @task.status %></h2>
</div>
